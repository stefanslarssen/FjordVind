# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Stripe Configuration (for payments)
# Get keys from https://dashboard.stripe.com/apikeys
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_xxxxx
# Price IDs - create products in Stripe Dashboard
VITE_STRIPE_PRICE_BASIC_MONTHLY=price_xxxxx
VITE_STRIPE_PRICE_BASIC_YEARLY=price_xxxxx
VITE_STRIPE_PRICE_PRO_MONTHLY=price_xxxxx
VITE_STRIPE_PRICE_PRO_YEARLY=price_xxxxx

# BarentsWatch API Credentials
# Register at https://www.barentswatch.no/minside/ to get credentials
VITE_BARENTSWATCH_CLIENT_ID=your-email@example.com:YourAppName
VITE_BARENTSWATCH_CLIENT_SECRET=your-client-secret

# Optional: API URL for backend
VITE_API_URL=http://localhost:3000
